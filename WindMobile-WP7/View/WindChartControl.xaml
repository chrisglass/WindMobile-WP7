<UserControl x:Class="Ch.Epyx.WindMobile.WP7.View.WindChartControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    Width="210" Height="210"
    d:DesignHeight="210" d:DesignWidth="210">
    
    <Grid x:Name="LayoutRoot">
        <Canvas x:Name="DrawCanvas" Margin="5">
            <Path x:Name="LinePath" Stroke="{StaticResource PhoneAccentBrush}" StrokeThickness="3" />
            <Ellipse Width="200" Height="200" Stroke="{StaticResource PhoneForegroundBrush}" />
            
        </Canvas>
    </Grid>
</UserControl>
